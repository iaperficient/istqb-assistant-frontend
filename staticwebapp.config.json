{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "routes": [
    {
      "route": "/auth/callback/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
